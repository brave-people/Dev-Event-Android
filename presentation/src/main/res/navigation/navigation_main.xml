<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="team.brave.devevent.android.presentation.fragment.dashboard.DashboardFragment"
        android:label="DashboardFragment"
        tools:layout="@layout/fragment_events">

        <argument
            android:name="isFavorite"
            android:defaultValue="false"
            app:argType="boolean" />

    </fragment>

    <fragment
        android:id="@+id/navigation_setting"
        android:name="team.brave.devevent.android.presentation.fragment.settings.SettingFragment"
        android:label="SettingFragment" />

</navigation>
